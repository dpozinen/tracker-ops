# Environment Configuration Template
# Copy this file to .env.development or .env.production and adjust values

# Enable/disable mock data mode
# Set to "true" for development with mock data (no backend required)
# Set to "false" for production with real API calls
VITE_USE_MOCK_DATA=true

# Mock API response delay in milliseconds (only used when VITE_USE_MOCK_DATA=true)
# Simulates network latency for realistic testing
VITE_MOCK_DELAY=800

# Backend API base URL
# Default: http://localhost:8133
VITE_API_BASE_URL=http://localhost:8133
